{
  "extends": "eslint:recommended",
  "env": {
    "browser": true,
    "commonjs": true
  },
  "globals": {
    "Promise": true,
    "Float32Array": true,
    "Uint8Array": true,
    "VERSION": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "sourceType": "module",
    "allowImportExportEverywhere": false
  },
  "rules": {
    "no-console": "off",
    "no-var": "error",
    "prefer-const": "error",
    "no-unused-vars": ["error", {"args": "none"}]
  }
}
