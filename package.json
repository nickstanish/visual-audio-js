{
  "name": "visual-audio-js",
  "version": "1.0.0",
  "description": "Audio Visualizer with WebGL",
  "scripts": {
    "start": "webpack-dev-server --config=webpack.dev.config.js  --hot --colors --compress --inline --watch",
    "build": "webpack --config=webpack.config.js",
    "clean": "rm -rf dist",
    "dist": "npm run clean && npm run build && mkdir -p dist && cp -r public index.html dist",
    "gh-pages": "git subtree push --prefix dist origin gh-pages",
    "lint": "eslint src",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nickstanish/visual-audio-js.git"
  },
  "keywords": [
    "webgl",
    "audio",
    "fft",
    "visualizer"
  ],
  "author": "Nick Stanish",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nickstanish/visual-audio-js/issues"
  },
  "homepage": "https://github.com/nickstanish/visual-audio-js#readme",
  "dependencies": {
    "gl-matrix": "2.4.0",
    "lodash.isequal": "^4.5.0",
    "soundcloud-audio": "^1.0.4"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-core": "^6.10.4",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "^0.23.1",
    "eslint": "^2.13.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "load-grunt-tasks": "^3.3.0",
    "raw-loader": "^0.5.1",
    "style-loader": "^0.13.1",
    "webpack": "^1.12.2",
    "webpack-dev-server": "^1.14.1"
  }
}
